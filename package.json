{
  "name": "react-plus",
  "version": "0.0.1",
  "description": "A collection of higher level react components.",
  "license": "MIT",
  "author": "Alex Bahnisch",
  "homepage": "https://github.com/alexbahnisch/react-plus#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/alexbahnisch/react-plus.git"
  },
  "bugs": {
    "url": "https://github.com/alexbahnisch/react-plus/issues"
  },
  "main": "./src/package/index.ts",
  "private": false,
  "keywords": [
    "react",
    "typescript"
  ],
  "dependencies": {
    "classnames": "^2.2.6"
  },
  "devDependencies": {
    "@types/classnames": "^2.2.6",
    "@types/node": "^10.11.7",
    "@types/react": "^16.4.16",
    "@types/react-dom": "^16.0.9",
    "awesome-typescript-loader": "^5.2.1",
    "copy-webpack-plugin": "^4.5.3",
    "cross-env": "^5.2.0",
    "css-loader": "^1.0.0",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "html-webpack-plugin": "^3.2.0",
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "tslint": "^5.11.0",
    "typescript": "^3.1.3",
    "webpack": "^4.20.2",
    "webpack-cli": "^3.1.2",
    "webpack-livereload-plugin": "^2.1.1"
  },
  "peerDependencies": {
    "react": "16.3",
    "react-dom": "16.3"
  },
  "scripts": {
    "clean": "npm run demo:clean && npm run package:clean",
    "demo:build": "npm run demo:clean && cross-env CONFIG=demo webpack --progress -p",
    "demo:clean": "rm -rf ./demo",
    "demo:watch": "cross-env CONFIG=demo NODE_ENV=development webpack --devtool inline-source-map --progress --watch",
    "lint:test": "tslint --project .",
    "lint:fix": "tslint --fix --project .",
    "package": "npm run package:compile && npm run package:bundle && npm run package:artifact",
    "package:artifact": "node ./scripts/pre-package.js && cd ./dist/ && npm pack ../build/",
    "package:bundle": "cross-env CONFIG=package MIN=true webpack -p && cross-env CONFIG=package webpack && rm ./build/zzz.*",
    "package:clean": "rm -rf ./build",
    "package:compile": "npm run package:clean && tsc --declaration",
    "publish": "cd ./build/ && npm publish"
  }
}